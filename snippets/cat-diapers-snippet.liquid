  <section class="ProductRelatedAdvertisement last_banner_pro {% if section.settings.specialProdAdvetImgmobile %} desktopSection {% endif %}">
    <div class="container">
      <div class="row">
        <div class="pp-6">
          <div class="leftRelatedAdvert">
            <h4>{{section.settings.specialProdAdvet}}</h4>
            <img class="lazyloaded lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.specialProdAdvetImg | img_url: 'master' }}" alt="RelativeCatDiapers">
            
          </div>
        </div>
        <div class="pp-6"> 
          <div class="rightRelatedAdvert Kat_diaper_add">
            <h4 class="red" style="color:#d55856 !important">{{section.settings.specialProdAdvetHead}}</h4>
            {% if section.settings.specialProdAdvetImg2 %}
            	<img class="lazyloaded lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.specialProdAdvetImg2 | img_url: 'master' }}" alt="useCase">
              
            {% endif %}
            {% comment %}adding Product_ids for inline cart{% endcomment %}
            {% assign productIds  = "" | split: "," %}
            {% if section.blocks.size > 0 %}
            	{% for block in section.blocks %}
                  {% if block.type == 'product' %}
                  	{% assign pro_data 		= block.settings.inline_product %}
                  	{% assign productt 		= all_products[pro_data] %}
            		{% assign stickyprice 	= stickyprice | plus: productt.selected_or_first_available_variant.price %}
                  	{% assign productId 	= productt.selected_or_first_available_variant.id | split: ',' %}
            		{% assign productIds 	= productIds | concat:productId  %}
                  {% endif %}
                {% endfor %}
            {% endif %}
            <div class="learnMoreBtn" data-pro="{{-productIds | join: '|' -}}">     
              	{% if section.settings.specialProdAdvetcheck %}
              		<a href="{{section.settings.specialProdAdveturl}}" class="RedButton signUpBtn ashdajsl" style="background:#e3002b;">{{section.settings.specialProdAdvetBut}}</a>	
              	{% else %}         
					<a href="javascript:void(0);" class="RedButton signUpBtn redBack __productInline">{{section.settings.specialProdAdvetBut}}</a>
				{% endif %}
            </div> 
          </div>          
        </div>
      </div>
    </div>
  </section>
