  {% if section.settings.everyNeedSection %}
  	<section class="PeePadd--Products container">
      <h1 class="big_brwn">FOR EVERY NEED</h1>
      <div class="PeePadDta pe_pad_varait">
 	  {% for block in section.blocks %}
      	 {% if block.type == 'ProductImageTwo' %}
        <div class="PeePadImages--two">
          <div class="ProductPeePad--leftSection">
            {% if block.settings.prodBanner1 %}
            	<img class="lazyloaded lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner1 | img_url: 'master' }}" alt="prodBanner1">
                  
