            {% endif %}
        </div>
        {% endif %}
      	 {% if block.type == 'ProductImageSingle' %}
        <div class="peePadProduct--SimpleLastImage">
            {% if block.settings.prodBanner4 %}
          	<img class="lazyloaded lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner4 | img_url: 'master' }}" alt="prodBanner4">
                  
