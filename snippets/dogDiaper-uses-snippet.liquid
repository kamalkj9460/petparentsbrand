{% if section.settings.is_middle %}
<div class="white_space">
  {% if section.settings.addi_img1 %}
    <div class ="product_dog_bannerd" >
        <div class ="__product_Banner">
          <img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.addi_img1 | img_url: 'master' }}" alt="addi_img1">
        
        </div>
      </div>
    {% endif %}
   {% if section.settings.is_middle_video_sec %}  
     {% capture media_content %}
      {% if section.settings.mediaType == 'img' %}
          <div class="top_additional_sec_Img individ_img">
            <img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.topAddit_sec_Img | img_url: 'master' }}" alt="topAddit_sec_Img">
            
  		</div>
      {% else %}
          <div class="inline_video img" video="{{section.settings.sec_video.id}}" href="{{section.settings.sec_video }}">
            {% comment %}<div class="video_poster_img">{{ section.settings.video_poster | img_url:'master' | img_tag:'img-responsive' }}</div>{% endcomment %}
            <div class='embed-container'><iframe src='https://www.youtube.com/embed/{{section.settings.sec_video.id}}' frameborder='0' allowfullscreen></iframe></div>
          </div>
      {% endif %}  
     {% endcapture %}
      <div class="additional_sec_helpIssue pro_points" style="margin-top:0px;">
        <div class="top_additional_sec_issue">
          {% if section.settings.media_alignment == 'left' %}{{-media_content-}}{% endif %}        
            <div class="top_additional_sec_content listing_pnts">
               {% if section.settings.topAddit_sec_head != blank %}<h2 class="red" style="color:{{section.settings.peePadHeaderColor}}!important ;font-weight: 500;letter-spacing: 3px;">{{section.settings.topAddit_sec_head}}</h2>{% endif %}
                <div class="top_additional_sec_subcontent points_lite">
                  <div class="top_add_sec_leftcontent">{{section.settings.addit_leftcontent}}</div>
                  <div class="top_add_sec_rightcontent">{{section.settings.addit_rightcontent}}</div>
                </div>
            </div>
          {% if section.settings.media_alignment == 'right' %}{{-media_content-}}{% endif %}
        </div>
        <p class="sub_cod">{{section.settings.addit_sec_paragraph}}</p>
      </div>
      {% style %}
                {% if section.settings.mediaType == 'video' %}
                  .embed-container { position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; max-width: 100%; margin-right: 30px; } .embed-container iframe, .embed-container object, .embed-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }
                  .top_additional_sec_subcontent{flex-direction: column;}
                  .inline_video { flex: 1 0 61%;}
                  .top_additional_sec_subcontent>div ul>li:before{ top:12px; }
                  .top_additional_sec_subcontent ul li { font-size: 23px !important; line-height: 34px !important; display: flex; margin: 0; align-items: center; align-self: center; }
                  .top_additional_sec_content.listing_pnts h1 { font-size: 28px; margin: 0px 0;}
                  p.sub_cod {padding: 30px 30px 0 30px;}
                  .inline_video{position:relative;cursor:pointer;}
                  .mfp-iframe-holder .mfp-close { top: -66px; right: -66px; }
                  /*.inline_video:after{ content:' ';background:url('{{ 'player.png' | file_img_url:'master'}}'); height: 20%; width: 10%; position: absolute; background-size: 100%; background-repeat: no-repeat; left: 0; right: 0; border: 0; margin: 0 auto; transform: translateY(54%); top: 29%; }*/
                  .mfp-bg {background-color: #0b0b0b;}
                  .inline_video.playing:after{opacity: 0;top: -100%;}
                  @media screen and (max-width:1020px){
                    .mfp-iframe-holder .mfp-close {top: -71px; right: -33px;}
                    .top_additional_sec_content.listing_pnts h1 { font-size: 25px;}
                    p.sub_cod { padding: 28px 0px 0 0px; }
                  }
                  @media screen and (max-width:767px){.embed-container{margin-left:none;}
                    .top_additional_sec_content.listing_pnts{order: 1;}
                    .inline_video { margin-top: 30px!important; }
                  }
                  @media screen and (min-width:768px) and (max-width:1023px){
                    .top_additional_sec_subcontent {flex-direction: row;}  			
                  }
                {%  endif %}
      {% endstyle %}
  {% else %}
  <div class="additional_sec_helpIssue pro_points points_40_60">
    <div class="top_additional_sec_issue">
      <div class="top_additional_sec_Img">
        <img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.topAddit_sec_Img | img_url: 'master' }}" alt="topAddit_sec_Img">
      	
      </div>
      <div class="top_additional_sec_content listing_pnts sf_heading">
         <h2 class="red">{{section.settings.topAddit_sec_head}}</h2>
        <div class="top_additional_sec_subcontent">
          <div class="top_add_sec_leftcontent">
              {{section.settings.addit_leftcontent}}
          </div>
          <div class="top_add_sec_rightcontent">
              {{section.settings.addit_rightcontent}}
          </div>
        </div>
      </div>      
    </div>
    <p class="sub_cod">{{section.settings.addit_sec_paragraph}}</p>
  </div>
  {% endif %}
