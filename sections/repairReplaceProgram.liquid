section#MakeAclameProgram {
    padding: 65px 0 30px;
    background: #f6f6f6;
}
#MakeAclameProgram h2 {
    padding: 0 0 25px;
}  
.repairBanner p {
    font-size: 20px;
    color: #706f6f;
    margin-bottom: 25px;
}  
.lster span {
    min-width: 22px;
    height: 22px;
    background: #00b6bd;
    font-size: 16px;
    font-weight: 400;
}  
.lster {
    align-items: start;
}  
.repairBanner p:last-child {
    margin-left: 42px;
    font-size: 16px;
    margin-bottom: 42px;
    font-style: italic;
}  
.btttn a {
    font-size: 17px;
    background: #00b6bd;
    padding: 8px 16px;
    color: #fff;
    text-decoration: none;
    border-radius: 20px;
    display: block;
    width: max-content;
    margin: 0px auto;
    line-height: 1;
    transition: .25s;
    height: 36px;
} 
.btttn a:hover {
    background: #ed1b3c;
} 
.repairRSlide_items {
    display: grid;
    grid-template-columns: 1fr 1fr;
    align-items: center;
    grid-gap: 75px;
}
.repairRSlide_items .rrs_pro_img {
    margin-left: auto;
}  
ul.slick-dots {
    display: none !important;
}  
.RepairRSlider {
    margin: 110px  0 15px !important;
}  

.slick-arrow {
    position: absolute;
    top: -120px;
    background: #e7e7e7 !important;
    left: unset;
    border-radius: 50%;
    height: 22px;
    width: 22px;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    min-height: 22px !important;
}
.slick-arrow:hover {
    background: #a4a5a5 !important;
}
.slick-next {
  	right: 5px;
}
.slide_prev {
  	right: 35px;
}
.slick-arrow:before {
    content: "";
    background-position: center;
    width: 15px;
    height: 15px;
    background-repeat: no-repeat !important;
    z-index: 9999;
    position: absolute;
    background-position: center center !important;
    width: 22px;
    height: 22px;
}  
.slide_prev:before {
    background: url(https://cdn.shopify.com/s/files/1/1355/5443/files/a_left.png);
}   
.slide_next:before {
    background: url(https://cdn.shopify.com/s/files/1/1355/5443/files/a_right.png);
}
.__Footer_sec_inner {
    border-top: 0;
}
#repairReplaceSlider h2 span::after {
    background: #ed1b3c;
    height: 15px !important;
    border-radius: 30px;
}  
#MakeAclameProgram h2 span::after {
    height: 15px  !important;
    border-radius: 30px;
}  
 
  
