    width: 100%;
}
.home-shrink-content {
    position: absolute;
    left: 0;
    right: 0;
    margin: 0px auto;
    height: 100%;
    top: 0;
    width: 100%;
}  
.home-shrink-content .row {
    height: 100%;
    align-items: center;
}  
.half-empty {
