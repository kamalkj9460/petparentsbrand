<section id="RepairReplaceProgram">
  <div class="RepairReplaceProgram RepairReplaceProgram_01" style="background:{{-section.settings.blc_banner_bg-}}">
  <div class="home-shrinkss">  
    
 {%comment%}   {% if section.settings.blc_banner_m %}
		{{ section.settings.blc_banner_m | img_url:'master' | img_tag:'blc_banner_m','home-blaket is_mobile_enble img-responsive' }}
	{% endif %}{%endcomment%}
      <div class="container">
          <div class="row ">
            <div class="half-empty">
              
                  {{ section.settings.blc_banner | img_url:'master' | img_tag:'blc_banner','img-responsive' }}
             
            </div>
              <div class="BannerHome">
                  <div class="BannerContent">
                    <h2>{{section.settings.blc_banner_heading}}</h2>
                    <p>{{section.settings.blc_banner_des}}</p>                    
                   </div>
              </div>
          </div>
      </div>
  </div>
  </div>
</section>
<section id="banner_bottom">
  <div class="container">
    <p>{{section.settings.blc_banner_b_content}}</p>
  </div>
</section>
<section id="MakeAclameProgram">
  <div class="container">    
 		<h2><span>{{section.settings.mc_heading}}</span></h2>
          <div class="repairBanner">
          {% for block in section.blocks %}
              {% if block.type == 'repairReplaceProgram' %}
          <p class="lster lster_0{{forloop.index}}"><span class="listNumber">{{block.settings.repairRytNo}}</span>{{block.settings.repairRyttext}}</p>
              {% endif %}
          {% endfor %}
          </div>
            <p class="btttn"><a href="{{section.settings.urlClaim}}">Make a Claim</a></p>
        </div>
</section>
<section id="repairReplaceSlider">
  <div class="container">
    <h2><span>{{section.settings.rr_heading}}</span></h2>
    <div class="RepairRSlider sf_PawtectBlanketSlider5 slider">
      {% for block in section.blocks %}
              {% if block.type == 'repairReplaceSlider' %}
          <div class="repairRSlide repairRSlide_0{{forloop.index}}">
            <div class="repairRSlide_items">
            {{ block.settings.rrs_pro_img | img_url:'master' | img_tag:'rrs_pro_img','img-responsive rrs_pro_img' }}
            {{ block.settings.rrs_chart_img | img_url:'master' | img_tag:'rrs_chart_img','img-responsive rrs_chart_img' }}
            </div>
      		</div>
              {% endif %}
          {% endfor %}
    </div>    
  </div>
</section>
{% schema %}
  {
    "name": "Repair Replace Program",
    "settings": [
	     {"type":"header", "content":"Banner"},
            {"type":"color","id":"blc_banner_bg","label":"Background","default":"#7facc4"},
            {"type":"image_picker","id":"blc_banner","label":"Image"},
            {"type":"image_picker","id":"blc_banner_m","label":"Image [Mobile]"},
            {"type":"text","id":"blc_banner_heading","label":"Banner Heading"},
            {"type":"html","id":"blc_banner_des","label":"Banner Description"},
			{"type":"html","id":"blc_banner_b_content","label":"Banner Bottom Content"},
	     {"type":"header","content":"Make a Claim"},
		 {"type":"text","id":"mc_heading","label":"Heading"},
		 {"type":"url","id":"urlClaim","label":"Link Make Claim"},
		{"type":"header","content":"Repair and Replacement"},
		 {"type":"text","id":"rr_heading","label":"Heading"}
	],
	"blocks" : [{
        "type": "repairReplaceProgram",
        "name": "Claim List",
        "settings": [
		 {"type":"text","id":"repairRytNo","label":"List Number"},
		 {"type":"text","id":"repairRyttext","label":"Add Text"}
		]},
		{
        "type": "repairReplaceSlider",
        "name": "Repair Replace Slide",
        "settings": [
		 {"type":"image_picker","id":"rrs_pro_img","label":"Product Image"},
		 {"type":"image_picker","id":"rrs_chart_img","label":"Chart Image"}
		]}],
	"presets":[{
      "name": "Repair Replace Program",
      "category": "Pages"
    }]

}
{% endschema %}

<style>
  body {font-family: "Gentona book" !important;}
