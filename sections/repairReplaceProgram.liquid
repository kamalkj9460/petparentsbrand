} 
.BannerContent h2 {
    font-size: 40px;
    font-family: "Gentona book" !important;
    color: #fff;
    font-weight: 500;
    text-transform: inherit;
    margin: 0;
    letter-spacing: 0;
    padding-bottom: 20px;
}
.BannerContent p {
    font-size: 20px;
    line-height: 1.3;
    margin-bottom: 30px;
}  
.home-shrinkss .row {
    align-items: center;
