<section id="wwa_Our_Mission">
<div class="container">
<div class="main_div">
<div class="wwa_Our_Mission _wwa_om_left">
{% if section.settings.wea_om_img %}
	
  	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.wea_om_img | img_url:'master' }}" alt="wea_om_img">
	{% endif %}
</div>
<div class="wwa_Our_Mission _wwa_om_right">
<h1 class="wwa_heading wwa_heading_blue">{{ section.settings.wea_om_title }}</h1>
<div class="_wwa_om_cont">{{ section.settings.wea_om_con }}</div>
</div>
</div>
</div>
</section>
<section id="wwa_Our_Principles">
<div class="container">
   <h2 class="wwa_heading wwa_heading_red">{{ section.settings.wea_op_title }}</h2>
<div class="op_main_div">
<div class="wwa_Our_Principles _wwa_op_left">
{{ section.settings.wea_op_c_left }}
</div>
<div class="wwa_Our_Principles _wwa_op_right">
{{ section.settings.wea_op_c_right }}
</div>
</div>
</div>
</section>
<section id="wwa_Our_Philanthropy">
<div class="container">
<div class="oph_main_div">
<div class="wwa_Our_Philanthropy _wwa_oph_left">
<h2 class="wwa_heading wwa_heading_blue">{{ section.settings.wea_oph_title }}</h2>
{{ section.settings.wea_oph_c_left }}
</div>
<div class="wwa_Our_Philanthropy _wwa_oph_right">
{% if section.settings.wea_oph_img %}
	
  	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.wea_oph_img | img_url:'master' }}" alt="wea_oph_img">
	{% endif %}
</div>
</div>
</div>
</section>

<section id="wwa_Our_Process">
<div class="container">
<div class="opro_main_div">
<h2 class="wwa_heading wwa_heading_red">{{ section.settings.wea_opr_title }}</h2>
<p>{{ section.settings.wea_opr_con }}</p>
</div>
  <div class="wwa_our_p_slider slider" id="wwa_our_process_slider">
    {% for block in section.blocks %} 
		{% if block.type == 'ProcessSlide' %} 
			<div class="opss_main_div" id="opss_main_div_{{forloop.index}}">
              <div class="opss_img">
                <span class="num_{{forloop.index}}">{{forloop.index}}</span>
                
                {% if block.settings.ps_img %}
                  
                	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.ps_img | img_url:'master' }}" alt="ps_img">
                  {% endif %}
              </div>
              <div class="opss_content">
                <h3>{{ block.settings.ps_title }}</h3>
                <p>{{ block.settings.ps_content }}</p>
              </div>
			</div>
		{% endif %}
	{% endfor %}
  </div>
  
</div>
</section>
<section id="wwa_Our_Vision" {% if section.settings.wea_ov_img != blank %} style="background:url(https://cdn.shopify.com/s/files/1/1355/5443/{{section.settings.wea_ov_img}});background-size: cover;background-repeat: no-repeat;background-position:center;"{% endif %}>
<div class="container">
<div class="ov_main_div">
<h2>{{ section.settings.wea_ov_title }}</h2>
<p>{{ section.settings.wea_ov_con }}</p>
</div>
</div>
</section>

{% schema %}
  {
    "name": "Who we are",
    "settings": [
            {"type":"header", "content":"Our Mission"},
            {"type":"image_picker","id":"wea_om_img","label":"Image"},
            {"type":"text","id":"wea_om_title","label":"Title"},
            {"type":"richtext","id":"wea_om_con","label":"Content"},
            {"type":"header", "content":"Our Principles"},
            {"type":"text","id":"wea_op_title","label":"Title"},
            {"type":"html","id":"wea_op_c_left","label":"Content Left"},
            {"type":"html","id":"wea_op_c_right","label":"Content Right"},
            {"type":"header", "content":"Our Philanthropy"},
            {"type":"text","id":"wea_oph_title","label":"Title"},
            {"type":"richtext","id":"wea_oph_c_left","label":"Content Left"},
            {"type":"image_picker","id":"wea_oph_img","label":"Image"},
            {"type":"header", "content":"Our Process"},
            {"type":"text","id":"wea_opr_title","label":"Title"},
            {"type":"html","id":"wea_opr_con","label":"Content"},
            {"type":"header", "content":"Our Vision"},
            {"type":"text","id":"wea_ov_title","label":"Title"},
            {"type":"richtext","id":"wea_ov_con","label":"Content"},
            {"type":"image_picker","id":"wea_ov_img","label":"Background Image"}
		],
		"blocks" : [{	
				"name":"Process Slide",
				"type":"ProcessSlide",
				"settings": [
				{"type":"image_picker","id":"ps_img","label":"Select Image"},
                {"type":"text","id":"ps_title","label":"Title"},
                {"type":"richtext","id":"ps_content","label":"Content"}
					]
				}]
  }
{% endschema %}
