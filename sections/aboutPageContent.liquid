<section id="sf_a_banner">
<div class="banner_row">
<div class="container">
<div class="sf_col_6 sf_left_area">
{% if section.settings.ab_img %}
	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.ab_img | img_url:'master' }}" alt="ab_img">
	{% endif %}
</div>
<div class="sf_col_6 sf_right_area">
  <h1 class="lg_heading">{{- section.settings.ab_title -}}</h1>
   <p>{{- section.settings.ab_description -}}</p>
</div>
</div>
</div>
</section>
{% for block in section.blocks %} 
		{% if block.type == 'imageAndContent' %} 
              <section id="sf_imageAndContent_{{forloop.index}}">
              <div class="ab_imageAndContent">
              <div class="container">
               <div class="sf_img_imageAndContent sf_col_6 {% if block.settings.text_float != blank %}{{- block.settings.text_float -}}{% endif %}">     
                 <p>{{- block.settings.ic_content -}}</p>
              </div>
              <div class="sf_text_imageAndContent sf_col_6">
              {% if block.settings.ab_block_image %}
                	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.ab_block_image | img_url:'master' }}" alt="ab_block_image">
                  {% endif %}
              </div>
              
              </div>
              </div>
              </section>
		{% endif %}
	{% endfor %}
<section id="sf_a_blue_box">
  <span class="tringle-btm"></span>
<div class="blue_box_row">
  <p>{{- section.settings.ab_blue_box -}}</p>
</div>
</section>
<section id="sf_our_mission">
<div class="our_mission_row">
<div class="container">
  <div class="sf_col_6 sf_left_our_mission mob_mission_img">   
{% if section.settings.om_img %}
    	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.om_img | img_url:'master' }}" alt="om_img">
	{% endif %}
</div>
  <h1 class="lg_heading">{{- section.settings.om_title -}}</h1>
  <div class="sf_l_our_mission">  
<div class="sf_col_6 sf_left_our_mission des_mission_img">   
{% if section.settings.om_img %}
  	<img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ section.settings.om_img | img_url:'master' }}" alt="om_img">
	{% endif %}
</div>
<div class="sf_col_6 sf_right_our_mission">
   <div>{{- section.settings.om_description -}}</div>
</div>
  </div>
  <p class="our_mission_f_text">{{- section.settings.om_footerText -}}</p>
</div>
</div>
</section>

{% schema %}
  {
    "name": "About Us",
    "settings": [
          {"type":"header","content":"Top Section"},
          {"type":"image_picker","id":"ab_img","label":"Image"},
          {"type":"text","id":"ab_title","label":"Title"},
          {"type":"html","id":"ab_description","label":"Description"},		 
          {"type":"header","content":"Blue Box"},
          {"type":"html","id":"ab_blue_box","label":"Content"},
          {"type":"header","content":"Our Mission"},
          {"type":"image_picker","id":"om_img","label":"Select Image"},
          {"type":"text","id":"om_title","label":"Title"},
          {"type":"html","id":"om_description","label":"Description"},
		  {"type":"html","id":"om_footerText","label":"Footer Text"}
	],
	"blocks" : [{
        "type": "imageAndContent",
        "name": "Image And Content",
        "settings": [
	       {"type":"html","id":"ic_content","label":"Content"},
			{"type":"select", "id":"text_float", "label":"Text Float", "options": [ {"value":"left", "label":"Left"},{"value":"right", "label":"Right" } ],"default":"left"},
	       {"type":"image_picker","id":"ab_block_image","label":"Image"}
		]} ],
	"presets":[{
      "name": "About Us",
      "category": "Pages"
    }]

}
{% endschema %}


<style>
p {
    font-size: 16px;
    color: #706f6f;
    padding-right: 40px;
font-family: 'Gentona Book';
}
 section#sf_a_banner .sf_col_6,.ab_imageAndContent .container .sf_col_6,section#sf_our_mission .sf_col_6{
    width: 100%;
}
section#sf_a_banner .banner_row .container,.ab_imageAndContent .container,section#sf_our_mission .sf_l_our_mission {
