    margin : 0;
    padding : 30px 0;
}
#sf_imageAndContent_1 .sf_text_imageAndContent img {
  	text-align: right;	
} 
section#sf_our_mission {
    padding-top: 70px;
}
#sf_our_mission .lg_heading {
    text-align: center;
    margin: 35px 0 70px;
}  
#sf_our_mission .lg_heading::after {
    left: 50%;
