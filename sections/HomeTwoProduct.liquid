<section class="BannerHome sf_Home_Two_Product deskTab">
  <div class="container">
		<div class="row">
          {% for block in section.blocks %} 
              {% if block.type == 'HomeProducts3' %} 
                  <div id="HomeProducts_des_{{forloop.index}}" class="BannerHome{% if block.settings.product_c_class != blank %} {{- block.settings.product_c_class -}}{% endif %}">              
                    <div class="BannerContent">
                      <h2>{{block.settings.product_title}}</h2>
                      <p>{{block.settings.product_des}}</p>
                      <a href="{{block.settings.product_url}}">Learn More</a>
                     </div>
                    <div class="shed_img">
                      <img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.product_image | img_url: 'master' }}" alt="product_image">
                     
                    </div>
				</div>
              {% endif %}
			{% endfor %}
	</div>
  </div>
</section>
<section class="BannerHome sf_Home_Two_Product mobBan">	
		<div class="row">
          <div class="proKnownSliderHome" id="sf_Home_Two_Product">
			{% for block in section.blocks %} 
              {% if block.type == 'HomeProducts3' %} 
                  <div id="HomeProducts_mob_{{forloop.index}}" class="BannerHome{% if block.settings.product_c_class != blank %} {{- block.settings.product_c_class -}}{% endif %}">              
                    <div class="BannerContent">
                      <h2>{{block.settings.product_title}}</h2>
                      <p>{{block.settings.product_des}}</p>
                      <a href="{{block.settings.product_url}}">Learn More</a>
                     </div>
                    <div class="shed_img">
                      <img class="img-responsive lazy3 lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.product_image | img_url: 'master' }}" alt="product_image">
                     
                    </div>
				</div>
              {% endif %}
			{% endfor %}
            </div>
	</div>
</section>

{% schema %}
{
"name" : "Home Two Product",
"settings" : [
],
"blocks" : [{	
				"name":"Add Product",
				"type":"HomeProducts3",
				"settings": [
				{"type":"image_picker","id":"product_image","label":"Product Image"},
                {"type":"text","id":"product_title","label":"Product Title"},
                {"type":"html","id":"product_des","label":"Product Description"},
                {"type":"url","id":"product_url","label":"Button Url"},
				{"type":"text","id":"product_c_class","label":"Custom Class"}
					]
				}],
"presets": [{
"name": "Home Products"
}]
}
{% endschema %}