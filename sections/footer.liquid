<!-- Fotter Section -->
<div class="container">
	<div class="row">
		<div class="white_space">
			<div class="__fotter_border02"></div>
		</div>
	</div>
</div>
<div class="__Footer_sec sf_footerSection">    
  <div class="container">     
	<div class="row">       
	  <div class="__Footer_sec_inner">
		<div class="col-md-4 foot_col_1">
		  <div class="footer01">
			<div class="footer_title">{{ section.settings.menu_title_1 | default:'PRODUCTS' }}</div>
			<ul class="footer_nav">
			  {% for link in linklists[section.settings.footer_link_list1].links %}
				<li><a href="{{ link.url }}">{{ link.title }}</a></li>
			  {% endfor %}
			</ul>
		  </div>
		</div>
        	<div class="col-md-5 foot_col_2">
		<div class="col-md-6">
		  <div class="footer01 footer01_f_child">
			<div class="footer_title">{{ section.settings.menu_title_2 | default:'COMPANY' }}</div>
			<ul class="footer_nav">
			  {% for link in linklists[section.settings.footer_link_list2].links %}
				<li><a href="{{ link.url }}">{{ link.title }}</a></li>
			  {% endfor %}
			</ul>
		  </div>  
		</div>
		<div class="col-md-6">
		  <div class="footer01 footer01_l_child">
			<div class="footer_title">{{ section.settings.menu_title_3 | default:'LEARN MORE' }}</div>
			<ul class="footer_nav">
			  {% for link in linklists[section.settings.footer_link_list3].links %}
				<li><a href="{{ link.url }}">{{ link.title }}</a></li>
			  {% endfor %}
			</ul>
		  </div>  
		</div>
        </div>
		<div class="col-md-3 foot_col_3">
		  <div class="footer02">         
			<div class="_paymentMethods">
<!-- 				<img class="lazy2" src="https://cdn.shopify.com/s/files/1/1355/5443/files/paw.png"> -->
                
              
              <img class="lazy2 paw_logo lazy3" data-src="{{ 'paw.svg' | asset_url }}" alt="The Soap Store">
			</div>
			<div class="new_icon_add_data">
				   <div class="join_new"> 					
                     <div class="clic_join">
						   <a href="https://petparentsbrand.com/pages/signup-form">Click for our email list</a>
						</div>
                     {% comment %}
					  <div class="clic_join blu_but">
						   <!-- a href="https://petparents.refersion.com/affiliate/registration">JOIN OUR AFFILIATE PROGRAM</a -->
                        	<a href="/pages/affiliate/">JOIN OUR AFFILIATE PROGRAM</a>
						</div>
                  {% endcomment %}
				   </div>

				   <div class="__social_icon icon_data_add">
					 {% if settings.social_facebook_link != blank %}
					 <a href="{{ settings.social_facebook_link }}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/0358/3377/4217/files/facebook.png?v=1584530778">
					 </a>
					 {% endif %}
					 {% if settings.social_instagram_link != blank %}
					 <a href="{{ settings.social_instagram_link }}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/0358/3377/4217/files/instagram.png?v=1584530778">
					 </a>
					 {% endif %}
					 {% if settings.social_youtube_link != blank %}
					 <a href="{{ settings.social_youtube_link }}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/0358/3377/4217/files/youtube.png?v=1584530779">
					 </a>
					 {% endif %}
					 {% if section.settings.social_linkedin_link != blank %}
					 <a href="{{section.settings.social_linkedin_link}}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/0358/3377/4217/files/linkdin.png?v=1584530778">
					 </a>
					 {% endif %}
                     {% if section.settings.social_tiktok_link != blank %}
					 <a href="{{section.settings.social_tiktok_link}}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/1355/5443/files/TikTokIcon-Grey.png?v=1645703047">
					 </a>
					 {% endif %}
                     {% if section.settings.social_pinterst_link != blank %}
					 <a href="{{section.settings.social_pinterst_link}}" target="_blank">
					   <img class="lazy2" data-src="https://cdn.shopify.com/s/files/1/1355/5443/files/PinterestIcon-Grey.png?v=1645703047">
					 </a>
					 {% endif %}
				   </div>
				 </div>
			<ul class="footer_nav">
				  <li class="copy_right">
					{% if section.settings.footer_mail != blank %}
					<a class="__footerMail" href="mailto:{{ section.settings.footer_mail }}">{{ section.settings.footer_mail }}</a><br/>
					{% endif %}
					&copy; {{ 'now' | date: '%Y' }} {{ section.settings.footer_cmpy_name }}<br>
					{{ section.settings.footer_all_right }}<br>
					{{ section.settings.owner_rights }}
				  </li>
				</ul>     
            
		  </div>  
		</div>
	  </div>
	</div>
	<div class="row">
	  <div class="__payment_title_img">
		<span>Payment Methods We Accept</span><img class="lazy2" data-src="{{ "payment_card.png" | asset_url }}" alt="Img" />
	  </div>
	</div>
  </div>
</div>
{% schema %}
{
	"name": "Footer",
	"settings": [
		{"type":"header","content":"Menu 1 Settings"},
		{"type":"text","id":"menu_title_1","label":"Menu Title","default":"Products"},
		{"type":"link_list","id":"footer_link_list1","label":"Choose menu"},

		{"type":"header","content":"Menu 2 Settings"},
		{"type":"text","id":"menu_title_2","label":"Menu Title","default":"Company"},
		{"type":"link_list","id":"footer_link_list2","label":"Choose menu"},

		{"type":"header","content":"Menu 3 Settings"},
		{"type":"text","id":"menu_title_3","label":"Menu Title","default":"Learn More"},
		{"type":"link_list","id":"footer_link_list3","label":"Choose menu"},

		{"type":"header","content":"Extra Settings"},
		{"type":"text","id":"menu_title_4","label":"Title","default":"GET CONNECTED"},
		{"type":"url","id":"social_linkedin_link","label":"LinkedIn Url"},
		{"type":"url","id":"social_tiktok_link","label":"TikTok Url"},
		{"type":"url","id":"social_pinterst_link","label":"Pinterest Url"},
		{"type":"text","id":"footer_cmpy_name","label":"Add company name","default":"Pet Parents LLC"},
		{"type":"text","id":"footer_all_right","label":"Footer All rights Text","default":"All Right Reserved."},
		{"type":"text","id":"footer_mail","label":"Footer Email_id"},
		{"type":"text","id":"owner_rights","label":"Add owner Right Text","default":"All Logos and Trademarks belong to their respective owners."}
	]
}
{% endschema %}
