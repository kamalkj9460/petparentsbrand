jQuery(document).ready(function(){
  if( jQuery(window).width() > 767 ){
    let counter = 1;
    let imgURL = 'https://cdn.shopify.com/s/files/1/1355/5443/files/';
    let acImg = jQuery('.Special_Features_img img').attr('src');
    jQuery('.Special_Features .inner_text,.Special_Features_row .inner_text').each(function(id, ele){

      let fname = '{{ product.handle }}-'+counter+'.png';
      jQuery(ele).attr({'data-src': fname ,'data-id': counter});

      if( 
        document.body.dataset.id == 6050703999166 
        || document.body.dataset.id == 626011668532 
        || document.body.dataset.id  == 625932894260 ){
        jQuery('.Special_Features_row .inner_text').last().addClass('dis');
      } 
      if( document.body.dataset.id == 626011668532 ){
        let height = jQuery('.Special_Features.Special_Featuresss').height();
        let count = jQuery('.Special_Features_content .inner_text').length;
        let cusHeight = height / count;
        jQuery('.Special_Features_content .inner_text').css('height', cusHeight+'px');
      }
      // create image and append to image div
      let image = imgURL+''+jQuery(ele).data('src');
      let img   = document.createElement('img');
      img.setAttribute('src', image);
      img.classList.add('hide');
      img.classList.add('img-'+counter);
      img.dataset.i = counter;
      jQuery('.Special_Features_img').append(img);


      jQuery(ele).on('mouseenter', function(){
        if( !jQuery(this).hasClass('dis') ){
          let ids = jQuery(ele).data('id');
          jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').addClass('black');
          jQuery(ele).addClass('white').removeClass('black');
          let img = imgURL+''+jQuery(ele).data('src');
          jQuery('.Special_Features_img').addClass('up_div');
          jQuery('.Special_Features_img img').addClass('hide');
          jQuery('.Special_Features_img .img-'+ids).removeClass('hide').addClass('updated');

        }else{
          jQuery('.Special_Features_img').removeClass('up_div');
          if( document.body.dataset.id == 626011668532){
            jQuery('.Special_Features_img img').addClass('hide');
            jQuery('.Special_Features_img img').eq(0).removeClass('hide');
            jQuery('.Special_Features_img img').eq(1).removeClass('hide');
            jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').removeClass('white black');
          }else{
            jQuery('.Special_Features_img img').eq(0).removeClass('hide');
            jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').removeClass('white black');
          }


        }
      });

      jQuery(ele).on('mouseleave', function(){
        if( !jQuery(this).hasClass('dis') ){
          jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').removeClass('white').addClass('black');
          jQuery('.Special_Features_img img').addClass('hide');   
          jQuery('.Special_Features_img').removeClass('up_div');
          if( document.body.dataset.id == 626011668532){            
            jQuery('.Special_Features_img img').eq(0).removeClass('hide');
            jQuery('.Special_Features_img img').eq(1).removeClass('hide');
          }else{
            jQuery('.Special_Features_img img').eq(0).removeClass('hide');
          }
        }
      });
      counter++;
    });
    jQuery('.Special_Features_img').on('mouseenter', function(){
      jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').removeClass('black');
      jQuery('.Special_Features_img').removeClass('up_div');
      jQuery('.Special_Features_img img').addClass('hide');
      if( document.body.dataset.id == 626011668532){      	
        jQuery('.Special_Features_img img').eq(0).removeClass('hide');
        jQuery('.Special_Features_img img').eq(1).removeClass('hide');
      }else{
        jQuery('.Special_Features_img img').eq(0).removeClass('hide');
      }
    });

    jQuery('.Special_Features, .Special_Features_row').on('mouseleave', function(){
      jQuery('.Special_Features .inner_text, .Special_Features_row .inner_text').removeClass('black white');
      jQuery('.Special_Features_img').removeClass('up_div');
      jQuery('.Special_Features_img img').addClass('hide');
      if( document.body.dataset.id == 626011668532){      	
        jQuery('.Special_Features_img img').eq(0).removeClass('hide');
        jQuery('.Special_Features_img img').eq(1).removeClass('hide');
      }else{
        jQuery('.Special_Features_img img').eq(0).removeClass('hide');
      }
    });
  }
});  

</script>

