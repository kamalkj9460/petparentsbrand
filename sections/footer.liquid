jQuery(document).on('change','._stickyPetOptions select.single-option-selector__radio.product-form__input', function() {

 var attrVal = jQuery(this).val();
 if(attrVal == 'Lodge'){
    jQuery("._stickyPetOptions").first().addClass('is_color_disable');
    jQuery("._stickyPetOptions").last().addClass('is_size_disable');
 }
 var colorVal = jQuery("._stickyPetOptions.is_color_disable select option:selected").val();
 if(colorVal == 'Lodge'){
    jQuery("._stickyPetOptions.is_size_disable select option:eq(1)").attr('selected',"selected");
    jQuery("._stickyPetOptions.is_size_disable select option").first().css('display','none');
