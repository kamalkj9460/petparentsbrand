  }else{
    jQuery("._stickyPetOptions.is_size_disable select option").first().css('display','block');
    jQuery("._stickyPetOptions.is_size_disable select option").last().css('display','block');
    jQuery("._stickyPetOptions.is_size_disable select option:eq(1)").attr('selected',"");
  }
}); 
  
