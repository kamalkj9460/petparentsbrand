    text-transform: capitalize;
    letter-spacing: 0;
    font-weight: 500;
    white-space: nowrap;
}
.pick-supli_items img {
    display: block;
    margin: 0px auto;
}
#SoftS_sliderrr .slick-track {
    display: inline-flex ;
    align-items: flex-end;
}
.pick-supli_items {
    padding: 0 15px;
}
.pick-wrapper-main {
    padding: 70px 0 20px;
}
.pick-supli_wrap a {text-decoration: none;}
.bc_slide img {
    height: 485px;width:100%;
    object-fit: cover;
}
.slick-slider .slick-dots li.slick-active button:before {
    background-color: #fff !important;
    border-color: #fff !important;
    width: 10px !important;
    height: 10px !important;
    margin: 0px !important;
}
.slick-slider .slick-dots li button:before {
    background-color: #ccc !important;
    border: 1px solid #ccc !important;
    width: 10px !important;
    height: 10px !important;
    margin: 0px !important;
}
.slick-slider .slick-dots {
    bottom: 25px;
}
.safety-txt h3.forEveryNeed {
    margin: 100px 0 35px;
} 
.safety_wrap {
    display: flex;
    align-items: stretch;
    justify-content: center;
    grid-column-gap: 25px;
    flex-wrap: wrap;
}
.safety_items {
    background: #f6f6f6;
    padding: 45px;
    display: flex;
    flex-direction: column;
    align-items: start;
    justify-content: start;
    width: 31.35%;
    grid-gap: 30px;
}
.safety_items>div>p {
    font-size: 20px;
    color: #706f6f;
}  
.safety_items>div>h2 {
    margin: 0;
