    padding-bottom: 30px;
}
.__Footer_sec_inner {
    border-top: none;
    padding: 0px 38px 40px;
}  

  
#SoftS_slider .slick-arrow {
    position: absolute;
    top: -90px;
    background: #e7e7e7;
    left: unset;
    border-radius: 50%;
    height: 22px;
    width: 22px;
    display: inline-flex;
    align-items: center;
    justify-content: center;
}
#SoftS_slider .slick-next {
    right : 0;
}
#SoftS_slider .slick-prev {
    right : 35px;
}
.slide_next, .slide_prev {
    position: absolute;
    top: -40px;
    background: #e7e7e7;
    left: unset;
