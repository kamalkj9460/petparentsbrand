}
.what_wrapper h2 {
    margin: 0;
    font-family: "Gentona";
    font-size: 20px;
    font-weight: 500;
    line-height: 1.25;
}
.what_wrapper p {
    margin: 5px 0 55px;
    font-family: "Gentona";
