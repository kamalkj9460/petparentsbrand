<style>
body {
    font-family: "Gentona";
}
.softsupps_bnr {
    margin-top: 20px;
}
.softsupps_inn {
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
}
.softsupps_inn img {
    margin: -105px 0 -170px;
}
.softsupps_txt {
    max-width: 245px;
    color: #f6f6f6;
    text-align: right;
}
.softsupps_txt p {
    font-size: 26px;
    line-height: 1.25;
}
.softsupps_txt h2 {
    font-size: 36px;
    font-weight: 900;
    font-family: "Gentona";
    margin: 0;
    margin-bottom: 15px;
}
h3.forEveryNeed {
    color: #706f6f;
    text-align: center;
    margin: 35px 0;
    font-family: "Gentona";
    font-size: 30px;
    text-transform: inherit;
    letter-spacing: 0;
    font-weight: 500;
}
.pick-supli_title {
  	color: #706f6f;
    text-align: center;
    margin: 20px 0 0;
    font-family: "Gentona";
