<div class="newformatingSizeSection sf_newformatingSizeSection">
  <div class="product-blanket-regular-middle-content">
  {%- for block in section.blocks -%}
     {%- case block.type -%}
        {%- when 'titleBB' -%}
           {%- if block.settings.show_block and block.settings.text != blank  -%}
              <div class="Spec_Features_txt block-{{block.id}} text-{{block.settings.text_align}}" {{block.shopify_attributes}}>
                 {{- block.settings.text -}}
              </div>
              {%- style -%}
                 .block-{{block.id}}{ background-color:{{block.settings.bg_color}}; }
                 .block-{{block.id}} p{font-size:{{ block.settings.font_size }}px; padding:{{block.settings.specingUp}}px {{block.settings.specingDown}}px; color:{{block.settings.font_color}};letter-spacing:{{block.settings.letter-spacing}}px;margin: 0;} 
              {%- endstyle -%}
           {% endif -%}
        {% when 'content_with_imgBB' %}
           {% if block.settings.show_block %}
              <section {{block.shopify_attributes}} class="additional_sec_helpIssue pro_points block-{{block.id}} {% if block.settings.block_type == 'block_type' %}white_space{% endif %}" style="background-color:{{block.settings.block_bg}}!important;">
                <div class="container"> 
                  <div class="top_additional_sec_issue">
                      <div class="top_additional_sec_Img sf_top_additional_left_sec" style="background:{{-block.settings.img__per_bg_clr-}}">
                         {{ block.settings.topAddit_sec_Img  | img_url:'master' | img_tag:'block-img','img-responsive' }}
                      </div>
                      <div class="top_additional_sec_content listing_pnts sf_top_additional_right_sec">
                         <h1 class="header-{{block.id}} text-{{block.settings.text_align}}">{{-block.settings.headerText-}}</h1>
                         <div class="top_additional_sec_subcontent">
                            <div class="top_add_sec_leftcontent">{{-block.settings.addit_leftcontent-}}</div>
                            <div class="top_add_sec_rightcontent">{{-block.settings.addit_rightcontent-}}</div>
                         </div>
                      </div>      
                   </div>
                </div>
                {% if block.settings.addit_sec_paragraph != blank %}
                 <p class="sub_cod text-{{block.settings.add_txt_align}}">{{-block.settings.addit_sec_paragraph-}}</p>
                {% endif %}
              </section>
              {% style %}        
                 .additional_sec_helpIssue.block-{{block.id}}{ 
                    margin:{{block.settings.margn-up}}px {{block.settings.margn-dwn}}px 0 {{block.settings.margn-dwn}}px !important; 
                    {% if block.settings.block_type == 'boxed' %}
                       width:{{ block.settings.width }}%;margin: 0 auto !important;
                    {% endif %}
                 }
                 {% if block.settings.img_align == 'right' %}
                    .block-{{block.id}} .top_additional_sec_Img{ order:1; }
                 {% endif %}
                 .header-{{block.id}}{
                    color: {{block.settings.headerTextColor}} !important;
                    font-size:{{block.settings.headerTextSize}}px !important;
                    letter-spacing:{{block.settings.ltr-spacing}}px !important;            
                 }         
              {% endstyle %}
           {% endif %}
        {% when 'cont_videoBB' %}  			
             {% if block.settings.enable_sec_video !=blank %}
                        <div class='embed-containerfg is_desktop_enble'> 
                         <video-js data-setup='{"controls": true, "autoplay": false, "preload": "auto"}' id="videoControl" class="video-js" width="100%" height="720" poster="https://cdn.shopify.com/s/files/1/1355/5443/{{-block.settings.video_poster-}}">
                           <source src="{{ block.settings.sec_video | asset_url }}" type="video/mp4">					
                        </video-js>
                        </div>
                        <div class='embed-containerfg is_mobile_enble'> 
                         <video-js data-setup='{"controls": true, "autoplay": false, "preload": "auto"}' id="videoControl" class="video-js" width="100%" height="720" poster="https://cdn.shopify.com/s/files/1/1355/5443/{% if block.settings.video_poster_m !=blank %}{{-block.settings.video_poster_m-}}{% else %}{{-block.settings.video_poster-}}{% endif %}">
                           <source src="{{ block.settings.sec_video | asset_url }}" type="video/mp4">					
                        </video-js>
                        </div>
              		{% endif %}  
            {% if block.settings.addit_sec_paragraph !=blank %}
              <div class="additional_sec_helpIssue pro_points never-secrifice" style="margin-top:0px; background-color:{{block.settings.addit_bg_color}} !important;">
                <div class="container">
                  <p class="sub_cod">{{block.settings.addit_sec_paragraph}}</p>
                </div>
              </div>
              {% endif %} 
        {% when 'sp_featureBB' %}
           {% if block.settings.show_block %}  
    <section class="sf_Special_Features_sec" style="background:{{ block.settings.block_bg_Spe}}">
			<div class="container">
              <div class="Special_Features">
	<div class="Special_Features_content">
		<div class="inner_text">
		<h3 spf_id="spc_ftu_1">{{ block.settings.featureTitle1}}</h3>
		<div id="spc_ftu_1" class="sf_c_content"><p>{{ block.settings.featureText1}}</p>		
		</div>{% if block.settings.featurePlusText1 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText1}}</p>{% endif %}</div>
		<div class="inner_text">
		<h3 spf_id="spc_ftu_2">{{ block.settings.featureTitle2}}</h3>
		<div id="spc_ftu_2" class="sf_c_content"><p>{{ block.settings.featureText2}}</p>
		</div>{% if block.settings.featurePlusText2 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText2}}</p>{% endif %}</div>
		
	</div>
	<div class="Special_Features_img" style="background:{{-block.settings.img__f_bg_clr-}}">
      	<img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.featureRightImg | img_url: 'master' }}" alt="featureRightImg">
		
	</div>
     
</div>
<div class="Special_Features_row">
  <div class="inner_text">
		<h3 spf_id="spc_ftu_3">{{ block.settings.featureTitle3}}</h3>
		<div id="spc_ftu_3" class="sf_c_content"><p>{{ block.settings.featureText3}}</p>		
		</div>{% if block.settings.featurePlusText3 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText3}}</p>{% endif %}</div>
		<div class="inner_text">
		<h3 spf_id="spc_ftu_4">{{ block.settings.featureTitle4}}</h3>
		<div id="spc_ftu_4" class="sf_c_content"><p>{{ block.settings.featureText4}}</p>		
		</div>{% if block.settings.featurePlusText4 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText4}}</p>{% endif %}</div>
	<div class="inner_text">
		<h3 spf_id="spc_ftu_5">{{ block.settings.featureTitle5}}</h3>
		<div id="spc_ftu_5" class="sf_c_content"><p>{{ block.settings.featureText5}}</p>		
		</div>{% if block.settings.featurePlusText5 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText5}}</p>{% endif %}</div>
	<div class="inner_text">
		<h3 spf_id="spc_ftu_6">{{ block.settings.featureTitle6}}</h3>
		<div id="spc_ftu_6" class="sf_c_content"><p>{{ block.settings.featureText6}}</p>		
		</div>{% if block.settings.featurePlusText6 != blank %}<p class="sf_plus_only">{{ block.settings.featurePlusText6}}</p>{% endif %}</div>
    
	</div>	
     
      </div>
</section>   
  			{% style %}
  		.__PeePadUse.block-{{block.id}} .row.row_iconic{ margin:0 auto;}
  			  {% endstyle %}
           {% endif %}
    {% when "gAntlerGuide" %}
                   <section class="AntlerGuide AntlerGuide-b-{{block.id}}" id="AntlerGuide-b-{{block.id}}">
                      <div class="container">
                          <div class="antler_row"><h3 class="forEveryNeed sliderEveryNeed">{{ block.settings.ag_heading }}</h3>
                              <div class="AntlerGuide-slider slider" id="AntlerGuide_sl">
                                  <div class="ag_slide_item ag_slide_item_01">
                                  <div class="ag_slide ag_slide_l">
                                    <h2>{{ block.settings.ag_bg_title_1 }}</h2>
                                    <span class="s_img deskTab">{{ block.settings.ag_bg_img_1 | img_url:'master' | img_tag:'ag_bg_img_1','img-responsive' }}</span>
                                    <span class="s_img mobBan">{{ block.settings.ag_bg_img__m_1 | img_url:'master' | img_tag:'ag_bg_img__m_1','img-responsive' }}</span>
                                  </div>
                                  <div class="ag_slide ag_slide_r">
                                  <p class="bottomTDes">{{ block.settings.ag_bg_des_1 }}</p>
                                  <p class="bottomTxt">{{ block.settings.ag_bg_btxt_1 }}</p>
                                  </div>
                                  </div>
                                 <div class="ag_slide_item ag_slide_item_02">
                                  <div class="ag_slide ag_slide_l">
                                    <h2>{{ block.settings.ag_bg_title_2 }}</h2>
                                    <span class="s_img deskTab">{{ block.settings.ag_bg_img_2 | img_url:'master' | img_tag:'ag_bg_img_2','img-responsive' }}</span>
                                    <span class="s_img mobBan">{{ block.settings.ag_bg_img__m_2 | img_url:'master' | img_tag:'ag_bg_img__m_2','img-responsive' }}</span>
                                  </div>
                                  <div class="ag_slide ag_slide_r">
                                  <p class="bottomTDes">{{ block.settings.ag_bg_des_2 }}</p>
                                  <p class="bottomTxt">{{ block.settings.ag_bg_btxt_2 }}</p>
                                  </div>
                                  </div>
                                 <div class="ag_slide_item ag_slide_item_03">
                                  <div class="ag_slide ag_slide_l">
                                    <h2>{{ block.settings.ag_bg_title_3 }}</h2>
                                    <span class="s_img deskTab">{{ block.settings.ag_bg_img_3 | img_url:'master' | img_tag:'ag_bg_img_3','img-responsive' }}</span>
                                    <span class="s_img mobBan">{{ block.settings.ag_bg_img__m_3 | img_url:'master' | img_tag:'ag_bg_img__m_3','img-responsive' }}</span>
                                  </div>
                                  <div class="ag_slide ag_slide_r">
                                  <p class="bottomTDes">{{ block.settings.ag_bg_des_3 }}</p>
                                  <p class="bottomTxt">{{ block.settings.ag_bg_btxt_3 }}</p>
                                  </div>
                                  </div>
                              </div>
                          </div>		
                      </div>
                  </section>
    {% when "AntlerGuideSize" %}
    <section class="AntlerGuideSize AntlerGuideSize-b-{{block.id}}" id="AntlerGuideSize-b-{{block.id}}">
                      <div class="container">
                        <h3 class="forEveryNeed sliderEveryNeed dasfdghfg">Antler Guide</h3>
                          <div class="row">                       
                            <div class="is_ags_mobile slider" id="AntlerGuideSize_mob">
                              
                                {% if block.settings.ags_img_m_1 %}
                                <div>{{ block.settings.ags_img_m_1 | img_url:'master' | img_tag:'ags_img_m_1','img-responsive' }}
                              </div>
                                {% endif %}
                              {% if block.settings.ags_img_m_2 %}
                                <div>{{ block.settings.ags_img_m_2 | img_url:'master' | img_tag:'ags_img_m_2','img-responsive' }}
                              </div>
                                {% endif %}
                              {% if block.settings.ags_img_m_3 %}
                                <div>{{ block.settings.ags_img_m_3 | img_url:'master' | img_tag:'ags_img_m_3','img-responsive' }}
                              </div>
                                {% endif %}
                 			 </div> 
                        </div>
                        </div>
    </section>
        {% when 'ProductImageTwoBB' %}
      {% if block.settings.show_block %}
    <section class="pb_slider_{{block.id}} ssss_slider">
    <h3 class="forEveryNeed sliderEveryNeed">{{ block.settings.slidertextBB }}</h3>
              <div class="sf_PawtectBlanketSlider5 slider slideriD_{{block.id}}" id="PawtectBlanketSlider5">    
                 {% if block.settings.prodBanner1 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner1 | img_url:'master' }}" alt="prodBanner1"></div>
                {% endif %}
                {% if block.settings.prodBanner2 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner2 | img_url:'master' }}" alt="prodBanner2"></div>
                {% endif %}
                {% if block.settings.prodBanner3 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner3 | img_url:'master' }}" alt="prodBanner3"></div>
                {% endif %}
                {% if block.settings.prodBanner4 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner4 | img_url:'master' }}" alt="prodBanner4"></div>
                {% endif %}
                {% if block.settings.prodBanner5 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner5 | img_url:'master' }}" alt="prodBanner5"></div>
                {% endif %}
                {% if block.settings.prodBanner6 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner6 | img_url:'master' }}" alt="prodBanner6"></div>
                {% endif %}
                {% if block.settings.prodBanner7 != blank %}
                <div class="bc_slide"><img class="img-responsive lazy2" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{ block.settings.prodBanner7 | img_url:'master' }}" alt="prodBanner7"></div>
                {% endif %}
              </div>
    </section>
           {% endif %}       
       
     {%- endcase -%}
  {%- endfor -%}
</div>
</div>
{% schema %}
  { 
    "name": "Gnawtlers Middle",
    "settings": [
    ],
	"blocks" : [
        {
           "type":"titleBB",
           "name":"Title",
           "settings":[
              { "type": "checkbox", "id": "show_block", "label": "Enable Block", "default": true },
              { "type": "richtext", "id": "text", "label": "Text", "default": "<p>Default Title</p>" }, 
              { "type": "select", "id": "text_align", "label": "Text Alignment", "options": [ { "value": "left", "label": "Left" }, { "value": "center", "label": "Center" }, { "value": "right", "label": "Right" } ], "default": "center" },
              { "type": "range", "id": "font_size", "min": 1, "max": 75, "step": 1, "unit": "px", "label": "Font Size", "default": 35 }, 
              { "type": "color", "id": "font_color", "label": "Font Color", "default": "#000000" }, 
              { "type": "color", "id": "bg_color", "label": "Block Background Color", "default": "#ffffff" },
              { "type": "range", "id": "specingUp", "min": 1, "max": 75, "step": 1, "unit": "px", "label": "Padding -  Top & Bottom", "default": 35 },
              { "type": "range", "id": "specingDown", "min": 1, "max": 75, "step": 1, "unit": "px", "label": "Padding -  Lfet & Right", "default": 35 },
      		  { "type": "range", "id": "letter-spacing", "min": 1, "max": 35, "step": 1, "unit": "px", "label": "letter spacing", "default": 15 }
           ]
        },
        {
           "type":"content_with_imgBB",
           "name":"Content with Image",
           "settings":[
              { "type": "header", "content": "Block settings" },
              { "type": "checkbox", "id": "show_block", "label": "Enable Block", "default": true },
              { "type": "select", "id": "block_type", "label": "Block Type", "options": [ { "value": "full", "label": "Full" }, { "value": "boxed", "label": "Boxed" } ], "default": "full" },
              { "type": "range", "id": "margn-up", "min": 0, "max": 100, "step": 1, "unit": "px", "label": "Top", "default": 15 },
              { "type": "range", "id": "margn-dwn", "min": 0, "max": 100, "step": 1, "unit": "px", "label": "Bottom", "default": 25 },
              { "type": "range", "id": "width", "min": 1, "max": 100, "step": 1, "unit": "%", "label": "Block Width", "default": 90, "info":"Applied if block Typle is boxed" },
              { "type": "color", "id": "block_bg", "label": "Block Background Color", "default": "#ffffff" },

              { "type": "header", "content": "Content settings" },
              { "type": "image_picker", "id": "topAddit_sec_Img", "label": "Block Image" },
			  { "type": "color", "id": "img__per_bg_clr", "label": "Image Background Color", "default": "#00b8bc" },
              { "type": "select", "id": "img_align", "label": "Img Alignment", "options": [ { "value": "left", "label": "Left" }, { "value": "right", "label": "Right" } ], "default": "left" },
              { "type": "text", "id": "headerText", "label": "Header Text" },
              { "type": "color", "id": "headerTextColor", "label": "Header Font Color", "default": "#81aec5" },
              { "type": "range", "id": "headerTextSize", "min": 1, "max": 75, "step": 1, "unit": "px", "label": "Header Font Size", "default": 44 },
              { "type": "range", "id": "ltr-spacing", "min": 1, "max": 35, "step": 1, "unit": "px", "label": "letter spacing", "default": 3 },
              { "type": "select", "id": "text_align", "label": "Text Alignment", "options": [ { "value": "left", "label": "Left" }, { "value": "center", "label": "Center" }, { "value": "right", "label": "Right" } ], "default": "center" },
               { "type": "html", "id": "addit_leftcontent", "label":"Content"},
               { "type": "html", "id": "addit_rightcontent", "label":"Content"},
              { "type": "text", "id": "addit_sec_paragraph", "label": "Addintonal Content" },
              { "type": "select", "id": "add_txt_align", "label": "Text Alignment", "options": [ { "value": "left", "label": "Left" }, { "value": "center", "label": "Center" }, { "value": "right", "label": "Right" } ], "default": "center" }   
           
			]
        },
        {
           "type":"cont_videoBB",
           "name":"Content with Video",
           "settings":[
              {"type": "checkbox", "id": "enable_sec_video","label": "Enable Video Section","default":true },
              	{ "type": "text", "id": "sec_video", "label": "Video URL","info":"Upload video in Assets folder and add file name with extensiton"}, 
				{ "type": "image_picker", "id": "video_poster", "label":"Poster Image" },
				{ "type": "image_picker", "id": "video_poster_m", "label":"Poster Image(Mobile)" },
				 {"type":"header","content":"Bottom Text"}, 
				{ "type": "color", "id": "addit_bg_color", "label": "Background Color", "default": "#f6f6f6" },
				{ "type":"html","id":"addit_sec_paragraph","label":"Content"}
			]
        },
        {
           "type":"sp_featureBB",
           "name":"Special Features",
           "settings":[
              { "type": "header", "content": "Block settings" },
              { "type": "checkbox", "id": "show_block", "label": "Enable Block", "default": true },
			  { "type": "color", "id": "block_bg_Spe", "label": "Block Background Color", "default": "#ffffff" },
			  {"type":"image_picker","id":"featureRightImg","label":"Right Image"},
			 { "type": "color", "id": "img__f_bg_clr", "label": "Image Background Color", "default": "#00b8bc" },
              {"type":"header", "content":"Section [1]"},
              {"type":"text","id":"featureTitle1","label":"Title"},
              {"type":"html","id":"featureText1","label":"Content"},
              {"type":"text","id":"featurePlusText1","label":"Plus Text"},
              {"type":"header", "content":"Section [2]"},
              {"type":"text","id":"featureTitle2","label":"Title"},
              {"type":"html","id":"featureText2","label":"Content"},
              {"type":"text","id":"featurePlusText2","label":"Plus Text"},
              {"type":"header", "content":"Section [3]"},
              {"type":"text","id":"featureTitle3","label":"Title"},
              {"type":"html","id":"featureText3","label":"Content"},
              {"type":"text","id":"featurePlusText3","label":"Plus Text"},
              {"type":"header", "content":"Section [4]"},
              {"type":"text","id":"featureTitle4","label":"Title"},
              {"type":"html","id":"featureText4","label":"Content"},
              {"type":"text","id":"featurePlusText4","label":"Plus Text"},
              {"type":"header", "content":"Section [5]"},
              {"type":"text","id":"featureTitle5","label":"Title"},
              {"type":"html","id":"featureText5","label":"Content"},
              {"type":"text","id":"featurePlusText5","label":"Plus Text"},
              {"type":"header", "content":"Section [6]"},
              {"type":"text","id":"featureTitle6","label":"Title"},
              {"type":"html","id":"featureText6","label":"Content"},
              {"type":"text","id":"featurePlusText6","label":"Plus Text"}
             
           ]
        },
        {
            "type": "ProductImageTwoBB",
            "name": "Slider",
            "settings": [
				{ "type": "checkbox", "id": "show_block", "label": "Enable Block", "default": true },
				{"type":"text","id":"slidertextBB","label":"Slider Heading"},
                {"type":"image_picker","id":"prodBanner1","label":"Slide 1"},
                {"type":"image_picker","id":"prodBanner2","label":"Slide 2"},
                {"type":"image_picker","id":"prodBanner3","label":"Slide 3"},
                {"type":"image_picker","id":"prodBanner4","label":"Slide 4"},
                {"type":"image_picker","id":"prodBanner5","label":"Slide 5"},
                {"type":"image_picker","id":"prodBanner6","label":"Slide 6"},
                {"type":"image_picker","id":"prodBanner7","label":"Slide 7"}
            ]
        },
		{
            "type": "gAntlerGuide",
            "name": "Antler Slider",
            "settings": [				
                      {"type":"text","id":"ag_heading","label":"Slider Heading"},
                      {"type": "header","content": "Slide 1"},
                      {"type":"image_picker","id":"ag_bg_img_1","label":"Background Image"},
                      {"type":"image_picker","id":"ag_bg_img__m_1","label":"Background Image(Mobile)"},
                      {"type":"text","id":"ag_bg_title_1","label":"Title"},
                      {"type":"html","id":"ag_bg_des_1","label":"Description"},
                      {"type":"html","id":"ag_bg_btxt_1","label":"Bottom Text"},
                      {"type": "header", "content": "Slide 2"},
                      {"type":"image_picker","id":"ag_bg_img_2","label":"Background Image"},
                      {"type":"image_picker","id":"ag_bg_img__m_2","label":"Background Image(Mobile)"},
                      {"type":"text","id":"ag_bg_title_2","label":"Title"},
                      {"type":"html","id":"ag_bg_des_2","label":"Description"},
                      {"type":"html","id":"ag_bg_btxt_2","label":"Bottom Text"},
                      {"type": "header", "content": "Slide 3"},
                      {"type":"image_picker","id":"ag_bg_img_3","label":"Background Image"},
                      {"type":"image_picker","id":"ag_bg_img__m_3","label":"Background Image(Mobile)"},
                      {"type":"text","id":"ag_bg_title_3","label":"Title"},
                      {"type":"html","id":"ag_bg_des_3","label":"Description"},
                      {"type":"html","id":"ag_bg_btxt_3","label":"Bottom Text"}
            		]
        },
		{
            "type": "AntlerGuideSize",
            "name": "Antler Slider (Mobile)",
            "settings": [					
                      {"type":"header","content":"Mobile"}, 
